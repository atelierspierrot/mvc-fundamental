{
    "name": "atelierspierrot/mvc-fundamental",
    "type": "library",
    "description": "A basic MVC system to use and extend easily.",
    "keywords": [ "library", "mvc", "front-controller" ],
    "homepage": "http://github.com/atelierspierrot/mvc-fundamental",
    "license": "GPL-3.0",
    "authors": [{
        "name": "Les Ateliers Pierrot & contributors",
        "email": "contact@ateliers-pierrot.fr",
        "homepage": "http://www.ateliers-pierrot.fr/"
    },{
        "name": "Pierre Cassat",
        "homepage": "http://e-piwi.fr/"
    }],
    "require": {
        "php": ">=5.4.0",
        "psr/log": "1.0.*",
        "atelierspierrot/patterns": "1.0.*",
        "atelierspierrot/library": "2.0.0-BETA",
        "atelierspierrot/service-container": "1.*",
        "atelierspierrot/http-fundamental": "1.*",
        "atelierspierrot/event-manager": "1.*"
    },
    "require-dev": {
        "sami/sami": "@stable",
        "phpunit/phpunit": "@stable",
        "phpmd/phpmd" : "@stable"
    },
    "autoload": { "psr-0": {  "MVCFundamental": "src" } },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    }
}
